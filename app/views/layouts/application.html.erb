<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <title> <%=h  yield(:title) || controller.action_name %> </title>
    <%= stylesheet_link_tag "application" %>

  </head>

  <body>

       
    <div class='navigation'>
		<%= render :partial => 'shared/navigation_bar'%>
    </div>

    <div class='content'>
       <%= yield %>
    </div>


    <%= javascript_include_tag "jquery-1.4.2","jquery-ui-1.8.2.custom.min",:cache=>'cache/jquery' %>
    <%= javascript_include_tag "application", :cache => 'cache/application' %>
    <%= render :partial=>"shared/orbited_settings"%>
    <%= render :partial=>"shared/xmpp_settings" %>
    <%= yield :js %>

  </body>
</html>

